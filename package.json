    "isomorphic-ws": "^4.0.1",
    "json-stable-stringify": "^1.0.1",
    "semver": "^7.5.1",
    "stream-browserify": "^3.0.0",
    "typescript": "^5.4.5",
    "vm-browserify": "^1.1.2",
    "webpack": "5.89.0",
