    "@holo-host/holo-key-manager-js-client": "^0.0.4",
    "@holochain/client": "0.16.3",
    "@msgpack/msgpack": "^2.7.1",
    "@types/k6": "~0.48.0",
    "@types/node": "^20.12.7",
    "@types/webpack": "5.28.5",
    "babel-loader": "9.1.3",
    "buffer": "^6.0.3",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "crypto-browserify": "^3.12.0",
